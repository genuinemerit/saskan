            "request": {
                "clients": ["player_avatar", "npc_avatar"],
                "replies": ["ack", "in_process", "reject"]
            },
            "response": {
                "key": "[client_id]~[service_name].response~[datetime]~[expiretime]~[uuid]",
                "router": "game_router",
                "gateway": "avatar_gate"
            }
        },
